package com.mycompany.gestaofuncionarios.states;

import com.mycompany.gestaofuncionarios.model.Funcionario;
import com.mycompany.gestaofuncionarios.presenter.FuncionarioPresenter;

public abstract class FuncionarioStates {
	private FuncionarioPresenter funcPresenter;
	private Funcionario func;
	
	public FuncionarioStates(FuncionarioPresenter funcPresenter, Funcionario func) {
		this.funcPresenter= funcPresenter;
		this.funcPresenter = func;
	}
	
	public abstract boolean statusState();
}
